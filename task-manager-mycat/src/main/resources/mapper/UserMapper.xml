<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.taskManager.mapper.UserMapper">

	<insert id="add" useGeneratedKeys="true" keyProperty="id">
			insert into user (name,indate)
				values(
					#{name},
					now()
				)
	</insert>
	
	<select id="find" resultType="com.cn.taskManager.model.User">
		select * from user
	</select>
	

</mapper>